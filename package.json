{
  "name": "react-st",
  "description": "My personal starter template for react-typescript",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@8.6.10",
  "main": "src/main.tsx",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test-nowatch": "cross-env CI=true vitest",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "i18next": "^23.2.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.2",
    "react-router-dom": "^6.8.1",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@evanryuu/eslint-config": "^0.1.10",
    "@iconify/react": "^4.1.1",
    "@iconify/types": "^2.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "typescript": "^5.1.6",
    "unocss": "^0.49.7",
    "vite": "^4.1.0",
    "vitest": "^0.33.0"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
